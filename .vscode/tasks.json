{
  "version": "2.0.0",
  "tasks": [
    {
        "label": "npm start task",
        "type": "shell",
        "command": "npm start --prefix front-end",
        "group": {
        "kind": "build",
        "isDefault": true
        },
        "isBackground": true,
        "problemMatcher": {
            "owner": "custom",
            "pattern": {
            "regexp": ".*"
        },
        "background": {
            "activeOnStart": true,
            // Shown when Angular dev server is compiling / starting
            "beginsPattern": "Compiling",
            // Shown when it’s ready – adjust to match your console output
            "endsPattern": "Compiled successfully"
        }

        }

    }
  ]
}